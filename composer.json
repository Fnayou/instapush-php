{
    "name": "fnayou/instapush-php",
    "type": "library",
    "description": "PHP wrapper for Instapush.im APIs",
    "keywords": [
        "instapush",
        "php",
        "api",
        "wrapper",
        "push",
        "notification"
    ],
    "homepage": "https://github.com/fnayou/instapush-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymen FNAYOU",
            "email": "fnayou.aymen@gmail.com",
            "homepage": "http://aymen-fnayou.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "fnayou/zeppelin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Fnayou\\Instapush\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fnayou\\Instapush\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "sniff": "phpcs src/ --standard=PSR2 --report=summary",
        "format": "phpcbf --standard=psr2 src/"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
